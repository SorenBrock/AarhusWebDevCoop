@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.News>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
}
<p><h1>@Umbraco.Field("header")</h1></p>
@Umbraco.Field("mainContent")
<ul>
    @foreach (var page in CurrentPage.Children.OrderBy("CreateDate desc"))
    {
        <li>
            <b>@page.title</b> (@page.CreateDate.ToString("MM/dd-yyyy HH:mm"))
            <dd>@page.info</dd>
            <br />
        </li>
    }
</ul>